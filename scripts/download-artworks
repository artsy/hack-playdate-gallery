. .env

for i in ${artworks[@]}; do
    echo $i
    wget --output-document=$i.jpg https://api.artsy.net/api/v1/artwork/$i/default_image/large.jpg?xapp_token=$XAPP_TOKEN
    mv $i.jpg support/artworks/$i.jpg
done

